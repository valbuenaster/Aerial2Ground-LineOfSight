    pointsJoint_Two.faces= 1+[ 1 11 9 ; 24 23 22 ; 32 47 2 ; 0 8 43 ; 32 53 54 ; 24 100 99 ; 45 14 13 ; 23 101 100 ; 20 19 139 ; 61 64 63 ; 57 68 67 ; 54 53 72 ; 50 49 76 ; 21 20 138 ; 28 96 95 ; 17 16 142 ; 26 98 97 ; 30 94 78 ; 48 31 78 ; 52 51 74 ; 55 70 69 ; 59 66 65 ; 29 95 94 ; 99 100 166 ; 18 17 141 ; 11 44 47 ; 62 63 143 ; 58 67 66 ; 55 54 71 ; 51 50 75 ; 25 99 98 ; 22 21 137 ; 23 22 136 ; 19 18 140 ; 60 65 64 ; 56 69 68 ; 53 52 73 ; 49 48 77 ; 16 15 143 ; 27 97 96 ; 0 1 2 ; 45 32 9 ; 7 82 81 ; 6 81 80 ; 5 80 79 ; 4 79 93 ; 3 93 92 ; 42 92 91 ; 91 90 40 ; 40 90 89 ; 89 88 38 ; 38 88 87 ; 87 86 36 ; 36 86 85 ; 85 84 34 ; 84 83 33 ; 102 135 134 ; 103 134 133 ; 104 133 132 ; 105 132 131 ; 106 131 130 ; 107 130 129 ; 108 129 128 ; 109 128 127 ; 110 127 126 ; 126 125 112 ; 125 124 113 ; 124 123 114 ; 123 122 115 ; 122 121 116 ; 121 120 117 ; 117 120 119 ; 142 174 173 ; 139 171 170 ; 96 97 163 ; 75 156 155 ; 71 152 151 ; 66 67 148 ; 98 164 163 ; 140 172 171 ; 141 173 172 ; 99 165 164 ; 68 149 148 ; 64 145 144 ; 75 76 157 ; 72 153 152 ; 96 162 161 ; 138 170 169 ; 94 160 159 ; 101 167 166 ; 73 154 153 ; 68 69 150 ; 64 65 146 ; 77 158 157 ; 63 144 175 ; 101 136 168 ; 143 175 174 ; 137 169 168 ; 77 78 159 ; 95 161 160 ; 73 74 155 ; 70 151 150 ; 66 147 146 ; 11 44 40 ; 12 91 90 ; 33 83 135 ; 119 82 7 ; 110 43 10 ; 46 128 127 ; 145 173 144 ; 12 9 11 ; 13 9 12 ; 14 9 13 ; 11 1 10 ; 1 0 10 ; 44 2 47 ; 44 43 2 ; 2 43 8 ; 47 32 46 ; 32 45 46 ; 32 31 2 ; 30 15 31 ; 20 25 21 ; 29 16 30 ; 28 27 18 ; 27 26 19 ; 26 25 20 ; 24 22 25 ; 10 0 43 ; 22 21 25 ; 16 15 30 ; 15 1 9 ; 29 28 17 ; 19 26 20 ; 18 27 19 ; 17 28 18 ; 16 29 17 ; 31 15 2 ; 15 1 2 ; 32 48 49 ; 62 15 9 ; 61 62 9 ; 60 61 9 ; 59 60 9 ; 58 59 9 ; 57 58 9 ; 56 57 9 ; 55 56 9 ; 32 31 48 ; 32 49 50 ; 32 50 51 ; 32 51 52 ; 32 52 53 ; 25 24 99 ; 46 45 13 ; 55 32 54 ; 55 9 32 ; 24 23 100 ; 138 20 139 ; 62 61 63 ; 58 57 67 ; 71 54 72 ; 75 50 76 ; 137 21 138 ; 29 28 95 ; 141 17 142 ; 27 26 97 ; 31 30 78 ; 77 48 78 ; 73 52 74 ; 56 55 69 ; 60 59 65 ; 30 29 94 ; 165 99 166 ; 140 18 141 ; 12 11 47 ; 15 62 143 ; 59 58 66 ; 70 55 71 ; 74 51 75 ; 26 25 98 ; 136 22 137 ; 101 23 136 ; 139 19 140 ; 61 60 64 ; 57 56 68 ; 72 53 73 ; 76 49 77 ; 142 16 143 ; 28 27 96 ; 8 0 2 ; 14 45 9 ; 6 7 81 ; 5 6 80 ; 4 5 79 ; 3 4 93 ; 42 3 92 ; 41 42 91 ; 41 91 40 ; 39 40 89 ; 39 89 38 ; 37 38 87 ; 37 87 36 ; 35 36 85 ; 35 85 34 ; 34 84 33 ; 103 102 134 ; 104 103 133 ; 105 104 132 ; 106 105 131 ; 107 106 130 ; 108 107 129 ; 109 108 128 ; 110 109 127 ; 111 110 126 ; 111 126 112 ; 112 125 113 ; 113 124 114 ; 114 123 115 ; 115 122 116 ; 116 121 117 ; 118 117 119 ; 141 142 173 ; 138 139 170 ; 162 96 163 ; 74 75 155 ; 70 71 151 ; 147 66 148 ; 97 98 163 ; 139 140 171 ; 140 141 172 ; 98 99 164 ; 67 68 148 ; 63 64 144 ; 156 75 157 ; 71 72 152 ; 95 96 161 ; 137 138 169 ; 78 94 159 ; 100 101 166 ; 72 73 153 ; 149 68 150 ; 145 64 146 ; 76 77 157 ; 143 63 175 ; 167 101 168 ; 142 143 174 ; 136 137 168 ; 158 77 159 ; 94 95 160 ; 154 73 155 ; 69 70 150 ; 65 66 146 ; 7 118 10 ; 44 43 33 ; 10 11 7 ; 44 33 34 ; 119 82 13 ; 44 34 35 ; 5 6 11 ; 35 36 44 ; 4 5 11 ; 44 36 37 ; 3 4 11 ; 44 38 39 ; 42 3 11 ; 44 39 40 ; 41 42 11 ; 11 6 7 ; 40 41 11 ; 83 135 47 ; 44 37 38 ; 84 83 47 ; 102 33 135 ; 85 84 47 ; 12 82 81 ; 86 85 47 ; 12 81 80 ; 87 86 47 ; 80 79 12 ; 88 87 47 ; 12 79 93 ; 89 88 47 ; 12 92 91 ; 90 89 47 ; 82 12 13 ; 47 12 90 ; 118 119 7 ; 12 93 92 ; 102 33 43 ; 10 118 117 ; 103 102 43 ; 10 117 116 ; 104 103 43 ; 10 116 115 ; 105 104 43 ; 10 115 114 ; 106 105 43 ; 10 114 113 ; 107 106 43 ; 10 113 112 ; 108 107 43 ; 10 112 111 ; 109 108 43 ; 111 110 10 ; 110 109 43 ; 46 47 135 ; 120 119 13 ; 121 120 13 ; 46 135 134 ; 122 121 13 ; 46 134 133 ; 123 122 13 ; 46 133 132 ; 124 123 13 ; 46 132 131 ; 125 124 13 ; 46 131 130 ; 126 125 13 ; 46 130 129 ; 127 126 13 ; 46 129 128 ; 13 46 127 ; 169 170 149 ; 171 172 147 ; 168 169 149 ; 153 166 152 ; 163 164 155 ; 161 162 157 ; 159 160 158 ; 161 157 160 ; 163 155 162 ; 165 153 164 ; 170 147 148 ; 166 167 152 ; 171 147 170 ; 145 146 173 ; 175 144 174 ; 162 156 157 ; 170 148 149 ; 172 146 147 ; 144 173 174 ; 146 172 173 ; 153 165 166 ; 160 157 158 ; 164 154 155 ; 150 151 167 ; 167 151 152 ; 168 150 167 ; 153 154 164 ; 150 168 149 ; 155 156 162 ];
    pointsJoint_Two.facecolor='r'; 
    
    pointsbccwr=pointsbccwl;
    pointsbcwf=pointsbccwl;
    pointsbcwb=pointsbccwl;
    
    bccwl=patch(pointsbccwl);
    bccwr=patch(pointsbccwr);
    bcwf=patch(pointsbcwf);
    bcwb=patch(pointsbcwb);
    
    J_0=patch(pointsJoint_Zero);
    J_1=patch(pointsJoint_One);
    J_2=patch(pointsJoint_Two);
    
    temp=hgtransform;
    hbccwl=hgtransform;
    hbccwr=hgtransform;
    hbcwf=hgtransform;
    hbcwb=hgtransform;
    hJ_0=hgtransform;
    hJ_1=hgtransform;
    hJ_2=hgtransform;
    set(bccwl,'parent',hbccwl);
    set(bccwr,'parent',hbccwr);
    set(bcwf,'parent',hbcwf);
    set(bcwb,'parent',hbcwb);
    set(J_0,'parent',hJ_0);
    set(J_1,'parent',hJ_1);
    set(J_2,'parent',hJ_2);
    
    hT1S = plotTrama([cos(0) -sin(0) 0;sin(0) cos(0) 0; 0 0 1],[0;0;0.035]);
    hTeeS = plotTrama([1 0 0;0 cos(pi) sin(pi);0 -sin(pi) cos(pi)],[0;0;0.0]);
    
    set(core,'parent',temp);
    set(hT1S,'parent',temp);
    set(hbcwf,'parent',temp);
    set(hbcwb,'parent',temp);
    set(hbccwr,'parent',temp);
    set(hbccwl,'parent',temp);
    set(hJ_0,'parent',temp);
    set(hJ_1,'parent',hJ_0);
    set(hJ_2,'parent',hJ_1);
    set(hTeeS,'parent',hJ_2);
    Q=struct('Body',temp,'hbcwf',hbcwf,'hbcwb',hbcwb,'hbccwr',hbccwr,'hbccwl',hbccwl,'J_0',hJ_0,'J_1',hJ_1,'J_2',hJ_2);
   
    Trama_0=[ eye(3) [X_OFFSET;Y_OFFSET;0];[0 0 0 1]];
    
    Rz=[cos(theta0)   -sin(theta0)   0;
        sin(theta0)    cos(theta0)   0;
                 0              0    1];
    Ry=[cos(alpha0)    0   sin(alpha0);
                 0     1            0;
       -sin(alpha0)    0   cos(alpha0)];
    Rx=[    1          0            0;
            0  cos(beta0)  -sin(beta0);
            0  sin(beta0)   cos(beta0)];
    R=Rx*Ry*Rz;
   
    Trama=[ R [x0;y0;z0];[0 0 0 1]];
   
    Tramabccwl=[ eye(3) [-scaling*r;0;0];[0 0 0 1]];
    Tramabccwr=[ eye(3) [scaling*r;0;0];[0 0 0 1]];
    Tramabcwf=[ eye(3) [0;scaling*r;0];[0 0 0 1]];
    Tramabcwb=[ eye(3) [0;-scaling*r;0];[0 0 0 1]];
   
    Rz1=[cos(gamma0+3*pi/4)   -sin(gamma0+3*pi/4)   0;
         sin(gamma0+3*pi/4)    cos(gamma0+3*pi/4)   0;
                         0                     0    1];    
    Trama_J_1=[ Rz1 [0;0;-0.093];[0 0 0 1]];
    
    Rx2=[    1         0          0;
             0  cos(phi0) -sin(phi0);
             0  sin(phi0)  cos(phi0)]; 
    Trama_J_2=[ Rx2 [0;0;0];[0 0 0 1]];   
    
    set(Q.Body,'Matrix',Trama);
    set(Q.hbccwl,'Matrix',Tramabccwl);
    set(Q.hbccwr,'Matrix',Tramabccwr);
    set(Q.hbcwf,'Matrix',Tramabcwf);
    set(Q.hbcwb,'Matrix',Tramabcwb);
    set(Q.J_0,'Matrix',Trama_J_1);
    set(Q.J_1,'Matrix',Trama_J_1);
    set(Q.J_2,'Matrix',Trama_J_2);
end